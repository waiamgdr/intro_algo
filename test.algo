REPEAT
i:=i+1;
    WHILE (str(i)==" ") DO
    c:=c+1
   END_WHILE

WHILE (str[i]==( a || o  ||i ||y ||u ||e ) DO
    a:=a+1;
END_WHILE

UNTIL (i==str.length+1); 

b:= str.length;
write(b);
write(c);
write(a);
END

